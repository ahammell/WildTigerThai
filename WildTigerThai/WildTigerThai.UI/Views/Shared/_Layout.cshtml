<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Wild Tiger Thai Restaurant</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Yanone+Kaffeesatz:400,700' rel='stylesheet' type='text/css'>
    <!-- Css -->
    <link rel="stylesheet" href="~/Content/nivo-slider.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/owl.theme.css">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.css" type='text/css'>
    <link rel="stylesheet" href="~/Content/style.css">
    <link rel="stylesheet" href="~/Content/responsive.css">
    <link rel="stylesheet" href="~/Content/Site.css" />
    @RenderSection("styles", false)
    <!--Scripts-->
    <script src="~/Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.nivo.slider.js" type="text/javascript"></script>
    <script src="~/Scripts/owl.carousel.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.nicescroll.js"></script>
    <script src="~/Scripts/jquery.scrollUp.min.js"></script>
    <script src="~/Scripts/main.js" type="text/javascript"></script>

</head>
<body>

    <!-- LOGO Start
    ================================================== -->

    <header>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                </div>	<!-- End of /.col-md-12 -->
            </div>	<!-- End of /.row -->
        </div>	<!-- End of /.container -->
    </header> <!-- End of /Header -->
    <!-- MENU Start
    ================================================== -->
    <nav class="navbar navbar-container ">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div> <!-- End of /.navbar-header -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav nav-main">
                    <li class="dropdown ">
                        <a href="#">
                            MENU
                            <ul class="dropdown-menu dropdown-menu-center ">
                                <li><a class="dropdown-menu-item">APPETIZERS</a></li>
                                <li><a class="dropdown-menu-item">STIR FRIED</a></li>
                                <li><a class="dropdown-menu-item">NOODLES</a></li>
                                <li><a class="dropdown-menu-item">CURRIES</a></li>
                                <li><a class="dropdown-menu-item">FRIED RICE</a></li>
                                <li><a class="dropdown-menu-item">SIDES</a></li>
                                <li><a class="dropdown-menu-item">MENU DOWNLOAD</a></li>

                            </ul>
                        </a>
                    </li>
                    <li><a href="#">SPECIALS</a></li>
                    <li class="active">
                        @Html.ActionLink("Wild Tiger Thai Restaurant and Bar", "Index", "Home", new { @class = "logo" })
                    </li>
                    <li><a href="~/Views/Home/Contact.cshtml">CONTACT</a></li>
                    <li class="dropdown">
                        <a href="#">
                            ACCOUNT
                            <ul class="dropdown-menu dropdown-menu-center ">
                                @Html.Partial("_loginPartial")
                            </ul>
                        </a>

                    </li>

                </ul> <!-- End of /.nav-main -->
            </div>	<!-- /.navbar-collapse -->
        </div>	<!-- /.container-fluid -->
    </nav>	<!-- End of /.nav -->
    <!-- SLIDER Start
    ================================================== -->
    @RenderBody()
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <div class="block clearfix">
                        <a href="#" class="footer-logo">
                            Wild Tiger Thai Restaurant and Bar
                        </a>
                        <p>
                            We are bringing new Thai flavor dishes to the South East of Vancouver, WA.
                        </p>
                        <h4 class="connect-heading">CONNECT WITH US</h4>
                        <ul class="social-icon">
                            <li>
                                <a class="facebook-icon" href="https://www.facebook.com/wildtigerthairestaurantofficialpage/" target="_blank">
                                    <i class="fa fa-facebook"></i>
                                </a>
                            </li>
                        </ul>	<!-- End Of /.social-icon -->
                    </div>	<!-- End Of /.block -->
                </div> <!-- End Of /.Col-md-4 -->
                <div class="col-md-4">
                    <div class="block">
                        <h4>GET IN TOUCH</h4>
                        <p>
                            <i class="fa  fa-map-marker"></i> <span>Food Code d.o.o.,</span>1825 SE 164th AVE Suite 101 VANCOUVER, WA 98683
                        </p>
                        <p> <i class="fa  fa-phone"></i> <span>Phone:</span> 360-882-8887 </p>
                        <p> <i class="fa  fa-mobile"></i> <span>Mobile:</span> 360-882-8887</p>
                    </div>	<!-- End Of /.block -->
                </div> <!-- End Of Col-md-3 -->
                <div class="col-md-4">
                    <div class="block">
                        <h4>UPCOMING ITEMS</h4>
                        <div class="media">
                            <a class="pull-left" href="#">
                                <img class="media-object" src="~/Content/Images/product-item.jpg" alt="...">
                            </a>
                            <a class="pull-left" href="#">
                                <img class="media-object" src="~/Content/Images/product-item.jpg" alt="...">
                            </a>
                            <a class="pull-left" href="#">
                                <img class="media-object" src="~/Content/Images/product-item.jpg" alt="...">
                            </a>
                            <a class="pull-left" href="#">
                                <img class="media-object" src="~/Content/Images/product-item.jpg" alt="...">
                            </a>
                            <a class="pull-left" href="#">
                                <img class="media-object" src="~/Content/Images/product-item.jpg" alt="...">
                            </a>
                            <a class="pull-left" href="#">
                                <img class="media-object" src="~/Content/Images/product-item.jpg" alt="...">
                            </a>
                        </div>	<!-- End Of /.media -->
                    </div>	<!-- End Of /.block -->
                </div> <!-- End Of Col-md-3 -->
            </div> <!-- End Of /.row -->
        </div> <!-- End Of /.Container -->
        <!-- FOOTER-BOTTOM Start
        ================================================== -->
        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <ul class="cash-out pull-left">
                            <li>
                                <a href="#">
                                    <img src="~/Content/Images/American-Express.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <img src="~/Content/Images/PayPal.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <img src="~/Content/Images/Maestro.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <img src="~/Content/Images/Visa.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <img src="~/Content/Images/Visa-Electron.png" alt="">
                                </a>
                            </li>
                        </ul>
                        <p class="copyright-text pull-right">Designed by <a href="http://www.themexpert.com">Themexpert</a> All Rights Reserved</p>
                    </div>	<!-- End Of /.col-md-12 -->
                </div>	<!-- End Of /.row -->
            </div>	<!-- End Of /.container -->
        </div>	<!-- End Of /.footer-bottom -->
    </footer> <!-- End Of Footer -->

    <a id="back-top" href="#"></a>
    @RenderSection("scripts", false)
</body>
</html>