@model IEnumerable<WildTigerThai.DATA.EF.Menu>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Menu1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SectionPrecedence)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Menu1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SectionPrecedence)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Section.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Menu_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Menu_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Menu_ID })
        </td>
    </tr>
}

</table>
